            <div class="d-flex align-items-center justify-content-between">
                <div class="d-flex align-items-center">
                    <a href="#">
                        <img src="@friend.User.ProfilePhotoUrl" alt="profile-img" class="img-fluid profile-img2">
                    </a>
                    <div class="friend-info ms-3">
                        <h5 id="@friend.User.UserId" class="clickable"  onclick="redirectToProfile(this.id)">@friend.User.Fullname</h5>
                        <p class="mb-0">15 friends</p>
                    </div>
                </div>
                <div class="card-header-toolbar d-flex align-items-center">
                    @if (friend.User.UserId != int.Parse(User.FindFirstValue(ClaimTypes.Authentication)))
                    {
                        @if (userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId || x.User2Id == friend.User.UserId) == null)
                        {
                            <span data-user-id="@friend.User.UserId" data-user-name="@friend.User.Fullname" class="btn btn-primary me-2 add-friend" role="button">
                                <i class="ri-add-line me-1 text-white"></i>Add Friend
                            </span>
                            @* <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton01"> *@
                            @*     <a class="dropdown-item" href="#">Unfriend</a> *@
                            @* </div> *@
                        }
                        else
                        {
                            bool? status = userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId || x.User2Id == friend.User.UserId) != null ? userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId || x.User2Id == friend.User.UserId)?.Status : false;
                            if (status is true)
                            {
                                <div class="dropdown">
                                    <span class="dropdown-toggle btn btn-secondary me-2" id="dropdownMenuButton03" data-bs-toggle="dropdown" aria-expanded="true" role="button">
                                        <i class="ri-check-line me-1 text-white"></i> Friend
                                    </span>
                                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton03">
                                        <a data-user-id="@friend.User.UserId" data-user-name="@friend.User.Fullname" class="dropdown-item cancel-friend" href="#">Unfriend</a>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="dropdown">
                                    <span class="dropdown-toggle btn btn-warning me-2" id="dropdownMenuButton02" data-bs-toggle="dropdown" aria-expanded="true" role="button">
                                        <i class="ri-time-line me-1"></i> Pending
                                    </span>
                                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton02">
                                        @{ bool? status2 = userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId) != null ? userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId).Status : false; }
                                        @if (status2 is false)
                                        {
                                            <a data-user-id="@friend.User.UserId" data-user-name="@friend.User.Fullname" class="dropdown-item cancel-friend" href="#">Cancel Pending</a>
                                        }
                                        else
                                        {
                                            <a data-user-id="@friend.User.UserId" class="dropdown-item accept-friend clickable">Confirm</a>
                                            <a data-user-id="@friend.User.UserId" data-user-name="@friend.User.Fullname" class="dropdown-item cancel-friend" href="#">Cancel</a>
                                        }
                                    </div>
                                </div>
                            }
                        }
                    }
                    else
                    {
                        <span class="btn btn-primary me-2" role="button">
                            <i class="ri-add-line me-1 text-white"></i>You
                        </span>
                    }
                </div>
            </div>
        </div>
    </div>
    }
