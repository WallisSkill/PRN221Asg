                                    <h5 id="@friend.User.UserId" class="clickable"  onclick="redirectToProfile(this.id)">@friend.User.Fullname</h5>
                                    <p class="mb-0">@friend.Count friends</p>
                                </div>
                            </div>
                            <div class="card-header-toolbar d-flex align-items-center">
                                @if (friend.User.UserId != int.Parse(User.FindFirstValue(ClaimTypes.Authentication)))
                                {
                                    @if (userfriends.FirstOrDefault(x => x.User1Id == friend.User.UserId || x.User2Id == friend.User.UserId) == null)
                                    {
                                        <span data-user-id="@friend.User.UserId" data-user-name="@friend.User.Fullname" class="btn btn-primary me-2 add-friend" role="button">
                                            <i class="ri-add-line me-1 text-white"></i>Add Friend
