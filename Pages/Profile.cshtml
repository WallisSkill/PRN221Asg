@page
@using System.Security.Claims
@using PRN221_Assignment.Data
@using PRN221_Assignment.Models
@model Profile
@{
    var user = (User)ViewBag.User;
    var photos = (List<Photo>)ViewBag.photos;
    var friends = (List<UserProfile>)ViewBag.friends;
    var userfriends = (List<Friend>)ViewBag.userFriends;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Profile]</title>
</head>
<body>
<div>
<div id="content-page" class="content-page">
<div class="container">
<div class="row">
<div class="col-sm-12">
    <div class="card">
        <div class="card-body profile-page p-0">
            <div class="profile-header">
                <div class="position-relative">
                    <img src="../assets/images/page-img/profile-bg1.jpg" alt="profile-bg" class="rounded img-fluid">
                    <ul class="header-nav list-inline d-flex flex-wrap justify-end p-0 m-0">
                        <li>
                            <a href="#">
                                <i class="ri-pencil-line"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <i class="ri-settings-4-line"></i>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="user-detail text-center mb-3">
                    <div class="profile-img">
                        <img src="@user.ProfilePhotoUrl" alt="profile-img" class="avatar-130 img-fluid"/>
                    </div>
                    <div class="profile-detail">
                        <h3 class="">@user.Fullname</h3>
                    </div>
                </div>
                <div class="profile-info p-3 d-flex align-items-center justify-content-between position-relative">
                    <div class="social-info">
                        <ul class="social-data-block d-flex align-items-center justify-content-between list-inline p-0 m-0">
                            <li class="text-center ps-3">
                                <h6>Posts</h6>
                                <p class="mb-0">690</p>
                            </li>
                            <li class="text-center ps-3">
                                <h6>Followers</h6>
                                <p class="mb-0">206</p>
                            </li>
                            <li class="text-center ps-3">
                                <h6>Following</h6>
                                <p class="mb-0">100</p>
                            </li>
                        </ul>
                    </div>
                    <div class="social-links">
                        <ul class="social-data-block d-flex align-items-center justify-content-between list-inline p-0 m-0">
                            @if (!User.FindFirstValue(ClaimTypes.Authentication).Equals(user.UserId.ToString()))
                            {
                                <li class="text-center">
                                    @if(userfriends.FirstOrDefault(x => x.User1Id == user.UserId || x.User2Id == user.UserId) == null){
                                        <span  data-user-id="@user.UserId" data-user-name="@user.Fullname" class="btn btn-primary me-2 add-friend" id="dropdownMenuButton01"  role="button">
                                            <i  class="ri-add-line me-1 text-white"></i>Add Friend
                                        </span>
                                    }
                                    else
                                    {
                                    bool? status = userfriends.FirstOrDefault(x => x.User1Id == user.UserId || x.User2Id == user.UserId) != null? userfriends.FirstOrDefault(x => x.User1Id == user.UserId || x.User2Id == user.UserId)?.Status :  false;
                                    if (status is true)
                                    {
                                    <div class="dropdown">
                                            <span class="dropdown-toggle btn btn-secondary me-2" id="dropdownMenuButton03" data-bs-toggle="dropdown" aria-expanded="true" role="button">
                                                <i class="ri-check-line me-1 text-white"></i> Friend
                                            </span>
                                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton03">
                                            <a data-user-id="@user.UserId" data-user-name="@user.Fullname" class="dropdown-item cancel-friend" href="#">Unfriend</a>
                                        </div>
                                    </div>
                                    }
                                    else
                                    {
                                    <div class="dropdown">
                                            <span class="dropdown-toggle btn btn-warning me-2" id="dropdownMenuButton02" data-bs-toggle="dropdown" aria-expanded="true" role="button">
                                                <i class="ri-time-line me-1"></i> Pending
                                            </span>
                                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton02">
                                            @{bool? status2 = userfriends.FirstOrDefault(x => x.User1Id == user.UserId)!= null? userfriends.FirstOrDefault(x => x.User1Id == user.UserId).User1Id== user.UserId: false;}
                                            @if (status2 is false)
                                            {
                                            <a data-user-id="@user.UserId" data-user-name="@user.Fullname" class="dropdown-item cancel-friend clickable" href="#">Cancel Pending</a>
                                            }
                                            
                                            else
                                            {
                                            <a data-user-id="@user.UserId" class="dropdown-item accept-friend clickable">Confirm</a>
                                            <a data-user-id="@user.UserId" data-user-name="@user.Fullname" class="dropdown-item cancel-friend" href="#">Cancel</a>
                                            }
                                        </div>
                                    </div>
                                    }
                                    }
                                </li>
                                <li class="text-center">
                                    <div class="">
                                        <span onclick="openChat('@user.UserId','@user.Fullname')" class="btn btn-primary me-2" role="button">
                                            <i class="ri-message-line me-1 text-white"></i>Message
                                        </span>
                                        @* <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton01"> *@
                                        @*     <a class="dropdown-item" href="#">Unfriend</a> *@
                                        @* </div> *@
                                    </div>
                                </li>
                            }
                            else
                            {
                                <li class="text-center">
                                    <div class="">
                                        <span class="btn btn-primary me-2" id="dropdownMenuButton01" data-bs-toggle="dropdown" aria-expanded="true" role="button">
                                            <i class="ri-pencil-line me-1 text-white"></i>Edit Profile
                                        </span>
                                        @* <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton01"> *@
                                        @*     <a class="dropdown-item" href="#">Unfriend</a> *@
                                        @* </div> *@
                                    </div>
                                </li>
                            }
                            
                        </ul>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="card">
        <div class="card-body p-0">
            <div class="user-tabing">
                <ul class="nav nav-pills d-flex align-items-center justify-content-center profile-feed-items p-0 m-0">
                    <li class="nav-item col-12 col-sm-3 p-0">
                        <a class="nav-link active" href="#pills-timeline-tab" data-bs-toggle="pill" data-bs-target="#timeline" role="button">Timeline</a>
                    </li>
                    <li class="nav-item col-12 col-sm-3 p-0">
                        <a class="nav-link" href="#pills-about-tab" data-bs-toggle="pill" data-bs-target="#about" role="button">About</a>
                    </li>
                    <li class="nav-item col-12 col-sm-3 p-0">
                        <a class="nav-link" href="#pills-friends-tab" data-bs-toggle="pill" data-bs-target="#friends" role="button">Friends</a>
                    </li>
                    <li class="nav-item col-12 col-sm-3 p-0">
                        <a class="nav-link" href="#pills-photos-tab" data-bs-toggle="pill" data-bs-target="#photos" role="button">Photos</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<div class="col-sm-12">
